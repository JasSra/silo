{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Pipeline": {
    "ContinueOnStepFailure": true,
    "CriticalSteps": [],
    "StepConfiguration": {
      "FileHashing": {
        "Enabled": true
      },
      "MalwareScanning": {
        "Enabled": false
      },
      "FileHashIndexing": {
        "Enabled": true
      }
    }
  },
  "AI": {
    "Enabled": true,
    "Provider": "OpenAI",
    "OpenAI": {
      "ApiKey": "your-openai-api-key-here"
    },
    "AzureOpenAI": {
      "Endpoint": "https://your-azure-openai.openai.azure.com",
      "ApiKey": "your-azure-openai-key-here",
      "Deployment": "gpt-4o",
      "TextDeployment": "gpt-4o-mini"
    }
  }
}
