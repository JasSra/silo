{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Pipeline": {
    "ContinueOnStepFailure": true,
    "CriticalSteps": [],
    "StepConfiguration": {
      "MalwareScanning": {
        "Enabled": false
      }
    }
  },
  "AI": {
    "Enabled": true,
    "Provider": "OpenAI",
    "OpenAI": {
      "ApiKey": "your-openai-api-key-here"
    }
  }
}
